{
    "name": "grayloon/geonames",
    "description": "PHP package for accessing the geonames.org API",
    "keywords": [
        "grayloon",
        "geonames",
        "API",
        "geo locator"
    ],
    "homepage": "https://github.com/grayloon/geonames",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Gray Loon Marketing Group",
            "email": "webmaster@grayloon.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "psr/http-message": "^1.0",
        "psr/cache": "^1.0",
        "php-http/httplug": "^1.1 || ^2.0",
        "php-http/discovery": "^1.0",
        "php-http/client-implementation": "^1.0",
        "php-http/client-common": "^1.6 || ^2.0",
        "php-http/cache-plugin": "^1.4"
    },
    "require-dev": {
        "cache/array-adapter": "^1.1.0",
        "friendsofphp/php-cs-fixer": "^2.16",
        "guzzlehttp/psr7": "^1.2",
        "larapack/dd": "^1.1",
        "php-http/guzzle6-adapter": "^1.0 || ^2.0",
        "php-http/mock-client": "^1.2",
        "phpunit/phpunit": "^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Grayloon\\Geonames\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Grayloon\\Geonames\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
